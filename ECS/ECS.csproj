<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <TargetFrameworks>netstandard2.0;net45</TargetFrameworks>
    <Version>0.9.1.1</Version>
    <Description>Entity Component System</Description>
    <Authors>DawidSobieszczuk</Authors>
    <Product>EntityComponentSystem</Product>
    <Copyright>Copyright DawidSobieszczuk 2018</Copyright>
    <PackageLicenseExpression>MIT</PackageLicenseExpression>
    <PackageProjectUrl>https://github.com/DawidSobieszczuk/EntityComponentSystem</PackageProjectUrl>
    <PackageReleaseNotes />
    <NeutralLanguage>en</NeutralLanguage>
    <PackageTags>ECS Game Engine Entity Component System Simple Dawid Sobieszczuk</PackageTags>
    <GeneratePackageOnBuild>true</GeneratePackageOnBuild>
    <PackageRequireLicenseAcceptance>false</PackageRequireLicenseAcceptance>
  </PropertyGroup>

  <ItemGroup>
    <PackageReference Include="Newtonsoft.Json" Version="12.0.1" />
    <PackageReference Include="Newtonsoft.Json.Bson" Version="1.0.2" />
  </ItemGroup>

  <ItemGroup>
    <Folder Include="Properties\" />
  </ItemGroup>

</Project>
